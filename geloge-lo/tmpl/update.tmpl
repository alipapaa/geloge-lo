<body onload="post_initialize()">
  <div style="margin: 0 auto; text-align: left;">
    <div class="viewer">
      <form action="javascript:startUI()">
        <input type="hidden" id="account" type="text" value="{{ screen_name }}"/>
        <div id="map_canvas"></div>
      </form>
    </div>
  </div>
  <div class="tweets">
    <input id="status" type="text"></input>
   <input type="button" value="post" onclick="javascript:press_post();"></input>
  </div>
